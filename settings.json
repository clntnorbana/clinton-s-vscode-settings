{
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "selection",
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.folding": false,
  "editor.glyphMargin": false,
  "editor.guides.indentation": false,
  "editor.wordWrap": "on",
  "editor.smoothScrolling": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.showFoldingControls": "mouseover",
  "editor.fontFamily": "Comic Code Light",
  "editor.padding.top": 8,
  // "editor.fontFamily": "FiraCode Nerd Font Mono",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.lineNumbers": "relative",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "visible",
  "editor.fontSize": 14,
  "editor.cursorWidth": 1,
  "editor.cursorBlinking": "solid",
  "workbench.colorCustomizations": {
    "editorGutter.background": "#00000000",
    "editorLineNumber.activeForeground": "#0ce755",
    "breadcrumb.background": "#00000000",
    "editorCursor.foreground": "#fff",
    "scrollbar.shadow": "#00000000"
  },
  // "workbench.editorAssociations": {
  //   "*.pdf": "default"
  // },
  // "workbench.list.defaultFindMode": "filter",
  // "workbench.settings.applyToAllProfiles": [],
  "workbench.layoutControl.enabled": false,
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.showTabs": "none",
  "workbench.editor.revealIfOpen": true,
  "workbench.iconTheme": "gruvbox-material-icon-theme",
  "workbench.colorTheme": "Kanagawa",
  "workbench.tree.renderIndentGuides": "none",
  "workbench.activityBar.location": "hidden",
  // "explorer.confirmDelete": false,
  // "explorer.confirmDragAndDrop": false,
  // "reactSnippets.settings.importReactOnTop": false,
  // "liveServer.settings.CustomBrowser": "chrome",
  // "liveServer.settings.donotShowInfoMsg": true,
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
  "css.lint.unknownProperties": "ignore",
  "css.lint.unknownAtRules": "ignore",
  "window.customTitleBarVisibility": "never",
  "window.commandCenter": false,
  "window.titleBarStyle": "native",
  // "extensions.ignoreRecommendations": true,
  // "launch": {
  //   "configurations": [],
  //   "compounds": []
  // },
  // "typescript.updateImportsOnFileMove.enabled": "always",
  // "javascript.updateImportsOnFileMove.enabled": "always",
  // "[javascript]": {
  //   "editor.defaultFormatter": "esbenp.prettier-vscode"
  // },
  // "[php]": {
  //   "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  // },
  // "[html]": {
  //   "editor.defaultFormatter": "esbenp.prettier-vscode"
  // },
  "apc.electron": {
    "frame": false,
    "titleBarStyle": "hidden",
    "opacity": 0.98
  },
  "apc.stylesheet": {
    ".title-label": "display: none !important",
    ".file-icon": "font-family: Comic Code Light; font-size: 12.5px;",
    ".folder-icon": "font-family: Comic Code Light; font-size: 12.5px;",
    ".monaco-workbench .monaco-list:not(.element-focused):focus:before": "outline: none; !important",
    ".sidebar": "padding-top: 10px !important; height: 100%; border-right: solid 1px !important",
    ".sidebar .composite.title": "display: none !important;",
    ".editor .split-view-view.visible:nth-child(2)": "border-left: solid  1px !important",
    ".letterpress": "display: none !important",
    ".monaco-breadcrumbs .label-name": "font-size: 12px",
    ".monaco-breadcrumb-item": "font-weight: 500; padding: 3px; font-family: Comic Code Light;",
    ".monaco-breadcrumbs": "display: flex !important; justify-content: center;",
    ".folder.monaco-breadcrumb-item": "background: #E8C872; color: #000 !important;",
    ".file.monaco-breadcrumb-item": "background: #FFF3CF; color: #000 !important; width: 100%",
    ".outline-element.monaco-breadcrumb-item": "background: #637A9F ; color: #fff !important;",
    ".title.show-file-icons": "position: absolute !important; bottom: 0; opacity: 0.8"
  },
  "multiCommand.commands": [
    {
      "command": "multiCommand.saveAndCloseEditor",
      "sequence": [
        "workbench.action.files.save",
        "workbench.action.closeActiveEditor"
      ]
    }
  ],
  "workbench.startupEditor": "none",
  "update.mode": "default"
}
